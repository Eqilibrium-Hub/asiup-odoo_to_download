<odoo>
    <data>
        <record id="mail_template_order_create" model="mail.template">
            <field name="name">New Order Create</field>
            <field name="model_id" ref="asiup_shopbase_integration.model_sale_order"/>
            <field name="subject">Your order ${object.name} has been received!</field>
            <field name="email_to">${object.partner_shipping_id.email_formatted | safe}</field>
            <field name="email_from">${(object.company_id.email or user.email) | safe}</field>
            <field name="lang">${object.partner_id.lang}</field>
            <field name="body_html" type="html">
                <div height="100%" align="center" width="100%"
                     style="font-family:Roboto,RobotoDraft,Helvetica,Arial,sans-serif;padding:20px 0px;color: #000; background-color:rgb(242,242,242);">
                    <div style="width: 600px; font-size: 17px;">
                        <div style="width: 100%; color: #444444;">
                            <div class="tmp-header" style="padding: 15px 0px 15px; background-color: #467bb8; width: 100%;">
                                <div style="display: table; width: 88%;">
                                    <div style="display: table-cell; font-size: 21px; color: #ffffff; text-align:left;">
                                        Happy Shop
                                    </div>
                                    %if object.date_order:
                                    <div style="display: table-cell; color: #ffffff; text-align: right;">
                                        ${object.date_order.strftime("%B %d, %Y")}
                                    </div>
                                    % endif
                                </div>
                            </div>
                            <div class="tmp-body" style="background-color: #ffffff;">
                                <div style="padding:30px 15px 5px; background-color:rgb(249, 249, 249)">
                                    <span style="display: block; width: max-content; height: max-content;"
                                          data-type="html/image" data-href="https://store.asiup.com/images/logo_email.png" data-alt="Happy Shop">
                                        <img style="max-width: 100%; width: 100px;" src="https://store.asiup.com/images/logo_email.png"/>
                                    </span>
                                    <div style="font-size:21px; padding-bottom: 6px;">
                                        <div>Order
                                            <span style="font-size: 25px; color: rgb(70,123,184);">#${object.name}</span>
                                        </div>
                                        <div>
                                            <span>Hi ${object.partner_shipping_id.first_name or 'our valued customer'}, Thank you for
                                                your purchase!
                                            </span>
                                            <br/>
                                            <span>
                                                <span>We are processing your order.</span>
                                            </span>
                                        </div>
                                    </div>
                                    <div style="text-align: left; padding:2px;
                                            display: block;
                                            margin: 0px;
                                            line-height: 30px;
                                            font-size: inherit;">
                                        <div style="padding-bottom: 5px;">
                                            <span>Kindly note that you have 6 hours to cancel or make changes to your order. If you want any
                                                information or support, please feel free to let us know by replying to this email.
                                            </span>
                                        </div>
                                        <div>
                                            <span>Once your order ships, we will email you the tracking information of your shipment.</span>
                                        </div>
                                    </div>
                                </div>
